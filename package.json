{
  "name": "pebble",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --tunnel",
    "dev": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios --tunnel",
    "web": "expo start --web",
    "tunnel": "expo start --tunnel",
    "lint": "expo lint",
    "lint:unused": "eslint . --ext .ts,.tsx",
    "check:knip": "knip",
    "check:tsprune": "ts-prune",
    "check:deps": "depcheck --ignores=@expo/*,expo-*,react-native-*",
    "check:all": "npm run check:knip && npm run check:tsprune && npm run check:deps",
    "test": "npm run test:build && node .tmp/tests/tests/run-tests.js",
    "test:build": "rm -rf .tmp/tests && tsc --project tsconfig.test.json",
    "seed": "node -e \"console.log('Seeding runs automatically during app initialization.')\""
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.4.0",
    "@expo/vector-icons": "^15.0.3",
    "@gluestack-style/react": "^1.0.57",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/react-native-aria": "^0.1.7",
    "@gluestack-ui/themed": "^1.1.73",
    "@react-native-aria/focus": "^0.2.9",
    "@react-native-aria/interactions": "^0.2.16",
    "@react-native-aria/overlays": "^0.3.15",
    "@react-native-aria/utils": "^0.2.12",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~54.0.0",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "lucide-react-native": "^0.542.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "wa-sqlite": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-import": "^2.32.0",
    "knip": "^5.63.0",
    "ts-prune": "^0.10.3",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "inline-style-prefixer": "6.0.1"
  },
  "private": true
}
